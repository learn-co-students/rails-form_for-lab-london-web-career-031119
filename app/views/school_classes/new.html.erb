<h1>School Class Form</h1>

<%= form_tag("/school_classes") do %>
   <%= label_tag('school_class_title', "Title") %>
   <%= text_field_tag('school_class_title') %>
  
   <%= label_tag('school_class_room_number', "Room number") %>
   <%= text_field_tag('school_class_room_number') %>
  
   <%= submit_tag "Create School class" %>
 <% end %>
